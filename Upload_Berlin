
from sqlalchemy import create_engine
import pymysql
import pandas as pd

user = ""
password = ""
host = "localhost"
database = "AIRBNB"
table = "Berlin"

engine = create_engine(f"mysql+pymysql://{user}:{password}@{host}/{database}")

df = pd.read_csv("----berlin_weekdays.csv")

df.to_sql(name=table, con=engine, if_exists="append", index=False)

print("Success!")
